#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main ()

{
 int suma,i,cont,N; 
 cout <<"ingrese N: ";
 cin>>N;
 suma=0;
 cont=1;
 i=1;
 while (i<=N)
 {
	 suma=suma+cont;
	 cont=cont+2;
	 i=i+1;
 }
 cout<<"mostrar suma: ";
 cout<<suma ;
 getch();
 }

