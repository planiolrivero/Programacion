// serienaturales3.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "Math.h"

using namespace std;



int main()
{
	int suma,N;
	float i;
	cout<< "ingresar el valor de N: ";
	cin>> N;
	suma=0;
	for (i=1;i<=N;i++)
		suma=suma+pow(i,2);
	cout<< "La suma de los "<<N<< " Primeros numeros es: " << suma;
	getch();
	return 0;
}

