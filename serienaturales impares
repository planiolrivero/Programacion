// serienaturales.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "Match.h"

using namespace std;



int main()
{
	int suma,i,N;
	cout<< "ingresar el valor de N: ";
	cin>> N;
	suma=0;
	for (i=2;i<=N;i=i+2)
		suma=suma+i;
	cout<< "La suma de los "<<N<< " Primeros numeros es: " << suma;
	getch();
	return 0;
}

